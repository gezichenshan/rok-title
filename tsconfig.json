{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "bundler",
    "lib": ["es2021", "esnext", "esnext.asynciterable", "dom"],
    "esModuleInterop": true,
    "allowJs": true,
    "strict": true,
    "noEmit": true,
    "types": ["@types/node"]
  },
  "exclude": ["node_modules", "output"]
}
